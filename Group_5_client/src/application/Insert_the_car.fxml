<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Insert_the_carController">
   <children>
      <AnchorPane prefHeight="400.0" prefWidth="488.0">
         <children>
            <Label fx:id="titleL" layoutX="37.0" layoutY="24.0" prefHeight="58.0" prefWidth="374.0" text="Insert the car ">
               <font>
                  <Font name="System Bold" size="40.0" />
               </font>
            </Label>
            <Label fx:id="carnumL" layoutX="37.0" layoutY="95.0" prefHeight="33.0" prefWidth="116.0" text="Car number :">
               <font>
                  <Font name="Arial Unicode MS" size="18.0" />
               </font>
            </Label>
            <TextField fx:id="carnumTf" layoutX="167.0" layoutY="95.0" prefHeight="33.0" prefWidth="177.0" />
            <Button fx:id="submitBn" layoutX="37.0" layoutY="142.0" mnemonicParsing="false" onAction="#Submit_insert_car_click" prefHeight="58.0" prefWidth="86.0" text="Submit" />
            <Button fx:id="cancelBn" layoutX="181.0" layoutY="142.0" mnemonicParsing="false" onAction="#Cancel_insert_car_click" prefHeight="58.0" prefWidth="86.0" text="Cancel" />
            <ImageView fitHeight="221.0" fitWidth="507.0" layoutX="-8.0" layoutY="229.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../banner_city.jpg" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
